# Docker 환경 설정
SPRING_PROFILES_ACTIVE=docker

# 데이터베이스 설정 (Docker 컨테이너명 사용)
MONGODB_HOST=mongo
MONGODB_PORT=27017
MONGODB_DATABASE=unimeet

# Redis 설정 (Docker 컨테이너명 사용)
REDIS_HOST=redis
REDIS_PORT=6379

# JWT 설정
JWT_SECRET=docker-jwt-secret-key-change-this-in-production-environment
JWT_EXPIRATION=86400000

# 로깅 설정
LOG_LEVEL_SECURITY=INFO
LOG_LEVEL_APP=INFO
LOG_LEVEL_ROOT=WARN

# 서버 설정
SERVER_PORT=8080